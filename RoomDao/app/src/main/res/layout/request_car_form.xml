<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00F1F3FB"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <TextView
        android:id="@+id/id_employee_C"
        android:layout_width="320dp"
        android:layout_height="43dp"
        android:layout_marginTop="44dp"
        android:text="Номер сотрудника "
        android:textColor="#B6EFE7"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <TextView
        android:id="@+id/email_employee_C"
        android:layout_width="360dp"
        android:layout_height="43dp"
        android:layout_marginTop="44dp"
        android:text="Почтовый адрес"
        android:textColor="#B6EFE7"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/id_employee_C"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/firstname_employee_C"
        android:layout_width="320dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:text="Имя"
        android:textColor="#B6EFE7"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/email_employee_C"
        app:layout_constraintTop_toBottomOf="@+id/email_employee_C"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/lastname_employee_C"
        android:layout_width="320dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:text="Фамилия"
        android:textColor="#B6EFE7"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/firstname_employee_C"
        app:layout_constraintTop_toBottomOf="@+id/firstname_employee_C"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/post_employee_C"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:text="Пост"
        android:textColor="#B6EFE7"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/lastname_employee_C"
        app:layout_constraintTop_toBottomOf="@+id/lastname_employee_C"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/avatar_C"
        android:layout_width="360dp"
        android:layout_height="60dp"
        android:layout_marginTop="8dp"
        android:text="Аватар"
        android:textColor="#B6EFE7"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/post_employee_C"
        app:layout_constraintTop_toBottomOf="@+id/post_employee_C"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/id_car_C"
        android:layout_width="141dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:hint="№  "
        android:textColor="#FFF"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/avatar_C"
        app:layout_constraintTop_toBottomOf="@id/avatar_C" />

    <TextView
        android:id="@+id/brand_c"
        android:layout_width="200dp"
        android:layout_height="43dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="4dp"
        android:hint="Марка"
        android:textColor="#FFF"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/id_car_C"
        app:layout_constraintTop_toBottomOf="@+id/id_car_C"
        app:layout_constraintVertical_bias="0.052" />
</androidx.constraintlayout.widget.ConstraintLayout>